{
  "name": "riba-docs-theme",
  "version": "0.0.0",
  "description": "Shopify theme based on Bootstrap 4 and Riba",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:ts": "npm run build:prod:ts && theme deploy --dir ./theme assets/main.bundle.js assets/vendors~main.bundle.js",
    "deploy:sass": "npm run build:prod:sass && theme deploy --dir ./theme assets/bundle.css",
    "test:ts": "tslint -c tslint.json 'src/**/*.ts' && tsc --noEmit",
    "test": "npm run test:ts",
    "build": "concurrently -k \"npm:build:prod:*\"",
    "build:prod:ts": "npm run test:ts && npm run webpack",
    "build:prod:sass": "node-sass --output-style compressed src/scss/bundle.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/bundle.css -o theme/assets/bundle.css",
    "build:prod:templates": "gulp build:templates",
    "build:dev": "concurrently -k \"npm:build:dev:*\"",
    "build:dev:ts": "npm run test:ts && npm run webpack:dev",
    "build:dev:sass": "npm run build:prod:sass",
    "build:dev:templates": "npm run build:prod:templates",
    "_build:prod:ts:babel": "babel src/ts/main.ts --extensions '.ts'",
    "zip": "gulp build:zip",
    "watch": "concurrently -k \"npm:watch:*\"",
    "watch:theme": "theme watch --dir './theme'",
    "watch:ts": "npm run webpack:dev -- --watch",
    "watch:sass": "nodemon -e scss -x 'npm run build:prod:sass'",
    "watch:templates": "gulp watch:templates",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "link": "(npm link @ribajs/core @ribajs/i18n @ribajs/shopify @ribajs/router @ribajs/shopify-tda) && (cd node_modules/@ribajs/shopify-tda && npm run link) && (cd node_modules/@ribajs/router && npm run link) && (cd node_modules/@ribajs/shopify && npm run link) && (cd node_modules/@ribajs/i18n && npm run link) && (cd node_modules/@ribajs/core && npm run link)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/docs-theme"
  },
  "keywords": [
    "Bootstrap4",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Documentation",
    "Docs"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ribajs/docs-theme/issues"
  },
  "homepage": "https://art-code-studio.myshopify.com",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/prismjs": "^1.16.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-array-includes": "^2.0.3",
    "babelify": "^10.0.0",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^4.1.1",
    "css-flip": "^0.5.0",
    "css-loader": "^3.2.0",
    "csso-cli": "^2.0.2",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fancy-log": "^1.3.3",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-zip": "^5.0.0",
    "html-loader": "^0.5.5",
    "jshint": "^2.10.2",
    "jstransformer-markdown-it": "^2.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "prismjs": "^1.17.1",
    "sass-loader": "^7.2.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^6.0.4",
    "tsify": "^4.0.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-stream": "^5.2.1",
    "yargs": "^13.3.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.5.5",
    "@ribajs/bs4": "^1.0.1",
    "@ribajs/core": "^1.0.1",
    "@ribajs/i18n": "^1.0.1",
    "@ribajs/router": "^1.0.1",
    "@ribajs/shopify": "^1.0.1",
    "@ribajs/shopify-tda": "^1.0.1",
    "@types/debug": "^4.1.5",
    "@types/jquery": "^3.3.31",
    "bootstrap": "^4.3.1",
    "debug": "^4.1.1",
    "jquery": "^3.4.1",
    "p-queue": "^6.1.0",
    "popper.js": "^1.15.0",
    "ribajs": "github:ribajs/riba#master",
    "rxjs": "^6.5.2",
    "three": "^0.107.0"
  }
}
