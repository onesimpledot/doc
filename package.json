{
  "name": "@ribajs/doc",
  "version": "1.8.0",
  "description": "Shopify theme for the Riba.js Documentation",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:ts": "npm run build:prod:ts && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.main.bundle.js",
    "deploy:sass": "npm run build:prod:sass && theme deploy --dir ./theme assets/bundle.css",
    "deploy:pug": "npm run build:prod:pug && theme deploy --dir ./theme layout sections snippets templates",
    "test:ts": "eslint src --ext .js,.jsx,.ts,.tsx --fix && tsc --noEmit",
    "test": "npm run test:ts",
    "build": "concurrently \"npm:build:prod:*\"",
    "build:prod:ts": "npm run test:ts && npm run webpack",
    "build:prod:sass": "node-sass --output-style compressed src/scss/bundle.scss -o theme/assets/ && postcss -c postcss.config.js theme/assets/bundle.css -o theme/assets/bundle.css",
    "build:prod:pug": "concurrently \"npm:build:prod:pug:*\"",
    "build:prod:pug:snippets": "pug -O ./pug.options.js src/pug/snippets -E liquid -o theme/snippets",
    "build:prod:pug:templates": "pug -O ./pug.options.js src/pug/templates -E liquid -o theme/templates",
    "build:prod:pug:layout": "pug -O ./pug.options.js src/pug/layout -E liquid -o theme/layout",
    "build:prod:theme_settings": "gulp build:theme_settings",
    "build:prod:assets": "gulp build:assets",
    "build:prod:assets:iconset": "gulp build:assets:iconset",
    "build:prod:assets:favicons": "gulp build:assets:favicons",
    "build:dev": "concurrently \"npm:build:dev:*\"",
    "build:dev:ts": "npm run test:ts && npm run webpack:dev",
    "build:dev:sass": "npm run build:prod:sass",
    "build:dev:pug": "concurrently \"npm:build:dev:pug:*\"",
    "build:dev:pug:snippets": "npm run build:prod:pug:snippets -- --pretty",
    "build:dev:pug:templates": "npm run build:prod:pug:templates -- --pretty",
    "build:dev:pug:layout": "npm run build:prod:pug:layout -- --pretty",
    "build:dev:theme_settings": "gulp build:theme_settings",
    "build:dev:assets": "gulp build:assets",
    "build:dev:assets:iconset": "gulp build:assets:iconset",
    "build:dev:assets:favicons": "gulp build:assets:favicons",
    "zip": "gulp build:zip",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:theme": "theme watch --dir './theme'",
    "watch:ts": "npm run webpack:dev -- --watch",
    "watch:sass": "nodemon -e scss -x 'npm run build:prod:sass'",
    "watch:pug": "concurrently \"npm:watch:pug:*\"",
    "watch:pug:snippets": "npm run build:dev:pug:snippets -- --watch",
    "watch:pug:templates": "npm run build:dev:pug:templates -- --watch",
    "watch:pug:layout": "npm run build:dev:pug:layout -- --watch",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:link": "npm link @ribajs/core @ribajs/i18n @ribajs/bs4 @ribajs/shopify @ribajs/router @ribajs/shopify-tda @ribajs/iconset @ribajs/extras"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/doc"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "Documentation",
    "Docs"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/doc/blob/master/README.md"
    }
  ],
  "bugs": {
    "url": "https://github.com/ribajs/doc/issues"
  },
  "url": "https://github.com/ribajs/doc",
  "homepage": "https://ribajs.com/",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-export-default-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime-corejs3": "^7.8.7",
    "@types/debug": "^4.1.5",
    "@types/prismjs": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-array-includes": "^2.0.3",
    "babelify": "^10.0.0",
    "clean-css-cli": "^4.3.0",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "css-flip": "^0.5.0",
    "css-loader": "^3.4.2",
    "csso-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "extract-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-print": "^5.0.2",
    "gulp-zip": "^5.0.1",
    "html-loader": "^0.5.5",
    "inspectpack": "^4.4.0",
    "jshint": "^2.11.0",
    "jstransformer-escape-html": "^1.1.0",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-prismjs": "^0.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.5.0",
    "prismjs": "^1.19.0",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.4.0",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-stream": "^5.2.1",
    "yargs": "^15.2.0"
  },
  "dependencies": {
    "@ribajs/bs4": "^1.8.0",
    "@ribajs/core": "^1.8.0",
    "@ribajs/extras": "^1.8.0",
    "@ribajs/i18n": "^1.8.0",
    "@ribajs/iconset": "^1.8.0",
    "@ribajs/router": "^1.8.0",
    "@ribajs/shopify": "^1.8.0",
    "@ribajs/shopify-tda": "^1.8.0",
    "bootstrap": "^4.4.1",
    "debug": "^4.1.1",
    "jquery": "^3.5.0",
    "lorem-ipsum": "^2.0.3",
    "popper.js": "^1.16.1",
    "ribajs": "github:ribajs/riba#master",
    "rxjs": "^6.5.4",
    "three": "^0.114.0"
  }
}
