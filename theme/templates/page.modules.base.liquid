
<div class="container py-5 page {{template|handleize}}" id="page-content-{{page.handle}}">
  <div class="row pt-5">
    <div class="col-12">
      <div class="row">
        <div class="col-12">
          <h1 class="page-title">{{ page.title }}
          </h1>
          <div class="rte">{{ page.content }}</div>
          <div class="content">
            <section class="my-5">
              <h2 id="binders">Binders</h2>
            </section>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>