<rv-example-tabs class="pt-3" handle="bs4-icon">
<template type="single-html-file">
<div rv-add-class='"["col-2", "col-3", "col-4", "col-5", "col-6"]" | parse | random'>Random column size</div>
</template>
</rv-example-tabs>
                </section>
